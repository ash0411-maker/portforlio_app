<!----- ツアー写真 ----->
<div class = "tour-image-wrapper clearfix">
	<span class = "book_mark">
		<div id = "book_marks_<%= @tour.id %>">
			<%= render 'tourist/book_marks/book_mark', tour: @tour %>
		</div>
	</span>
	<%= render 'partial/tour_image_wrapper', tour_photos: @tour_photos %>
</div>

<!------- ツアー情報 -------->
<div class = "tour-info-wrapper">
	<%= render 'partial/tour_info_wrapper', tour: @tour %>
</div>


<!-- ツアー情報詳細） -->
<div class = "tour-detail-wrapper">
	<%= render 'partial/tour_detail_wrapper', tour: @tour, chat_guide_ids: @chat_guide_ids, reviews: @reviews %>
</div>

<%= javascript_include_tag 'tour_show.js' %>
